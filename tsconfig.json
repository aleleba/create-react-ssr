{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "isolatedModules": false,
    "types": [
      "react/next", 
      "react-dom/next", 
      "node"
    ],
    "sourceMap": true,
    "baseUrl": "**/*",
    "outDir": "build",
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "include": [
    "**/*"
  ],
  "exclude": [
   "node_modules",
   "build",
   "PRNameGenerator.ts"
  ]
}